{
	"name": "ultimate-i18n-js",
	"version": "0.0.3",
	"description": "Ultimate internationalization library for web applications.",
	"repository": {
		"type": "git",
		"url": "https://github.com/b4rtaz/ultimate-i18n-js.git"
	},
	"main": "./lib/index.js",
	"types": "./lib/index.d.ts",
	"files": [
		"lib/index.js",
		"lib/index.d.ts",
		"lib/index.min.js"
	],
	"scripts": {
		"prepare": "cp -f ../README.md ./README.md && yarn build",
		"build": "rollup -c",
		"build:watch": "rollup -c --watch",
		"prettier": "prettier --check ./src",
		"prettier:fix": "prettier --write ./src"
	},
	"author": "b4rtaz",
	"license": "MIT",
	"devDependencies": {
		"prettier": "^2.7.1",
		"typescript": "^4.8.4",
		"rollup": "^2.79.1",
		"rollup-plugin-typescript2": "^0.34.1",
		"rollup-plugin-terser": "^7.0.2"
	},
	"keywords": [
		"i18n",
		"internationalization",
		"translation",
		"translate",
		"l10n"
	]
}
